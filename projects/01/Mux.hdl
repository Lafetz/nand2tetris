/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel , out=st1 );
    And(a=a , b=st1 , out=t1 );
    And(a=b , b=sel , out=t2 );
    Or(a=t1 , b=t2 , out=out );
    //// Replace this comment with your code.
}