/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a , out=t1 );
    Not(in=b , out=t2 );
    And(a=t1 , b=t2 , out=t3 );
    Not(in=t3 , out=out );
    //// Replace this comment with your code.
}