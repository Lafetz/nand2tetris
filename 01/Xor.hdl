/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a,out=nota);
    Not(in=b,out=notb);
    And(a=a,b=notb,out=tmp1);
    And(a=nota,b=b,out=tmp2);
    Or(a=tmp1,b=tmp2,out=out);
    //// Replace this comment with your code.
}